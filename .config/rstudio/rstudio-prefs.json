{
    "show_margin": false,
    "editor_keybindings": "vim",
    "soft_wrap_r_files": true,
    "syntax_color_console": true,
    "highlight_r_function_calls": true,
    "editor_theme": "Night Owl Customized from rsthemes",
    "document_author": "Michael Culshaw-Maurer",
    "rmd_chunk_input_inline": false,
    "save_workspace": "never",
    "jobs_tab_visibility": "shown",
    "show_doc_outline_rmd": true,
    "doc_outline_show": "sections_and_chunks",
    "full_project_path_in_window_title": true,
    "packages_pane_enabled": false,
    "initial_working_directory": "~",
    "rainbow_parentheses": true,
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "source_with_echo": true,
    "load_workspace": false
}